{
  "name": "tslint-checkstyle-reporter",
  "version": "1.0.0",
  "description": "A checkstyle reporter for TSLint",
  "main": "src/checkstyleFormatter.js",
  "dependencies": {
    "xml": "^1.0.0"
  },
  "devDependencies": {
    "tslint": "^3.1.1",
    "typescript": "^1.6.2"
  },
  "scripts": {
    "compile": "tsc -p .",
    "testLint": "tslint test/*.ts",
    "output": "tslint test/*.ts -s src -t xml --out output.xml"
  },
